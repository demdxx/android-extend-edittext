<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExtEditText">
        <attr name="validate">
            <flag name="regexp" value="0x0001" />
            <flag name="numeric" value="0x0002" />
            <flag name="alpha" value="0x0004" />
            <flag name="alphaNumeric" value="0x0008" />
            <flag name="email" value="0x0010" />
            <flag name="creditCard" value="0x0020" />
            <flag name="phone" value="0x0040" />
            <flag name="domainName" value="0x0080" />
            <flag name="ipAddress" value="0x0100" />
            <flag name="webUrl" value="0x0200" />
            <flag name="empty" value="0x0400" />
            <flag name="nocheck" value="0" />
        </attr>
        <attr name="errorString" format="string" />
        <attr name="emptyErrorString" format="string" />
        <attr name="validatorRegexp" format="string" />
        <attr name="auto" format="boolean" />
    </declare-styleable>
</resources>